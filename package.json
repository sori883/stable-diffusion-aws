{
  "name": "aws-cdk-template",
  "version": "0.1.0",
  "private": "true",
  "bin": {
    "aws-cdk-template": "bin/entrypoint.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:update": "jest -u",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy '*'",
    "deploy:never": "cdk deploy '*' --require-approval never",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "aws-cdk-lib": "2.173.2",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.2",
    "aws-cdk": "2.173.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.7.2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.12.0"
  }
}
